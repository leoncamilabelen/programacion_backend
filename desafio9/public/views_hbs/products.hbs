{{#if hayProductos}}
  <table>
    <thead>
      <th>Nombre</th>
      <th>Precio</th>
      <th>Foto</th>
    </thead>
    <tbody>
      {{#each productsList}}
        <tr>
          <td>{{this.title}}</td>
          <td>{{this.price}} </td>
          <td><img src="{{this.thumbnail}}" alt="{{this.title}}" /> </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
{{else}}
  <strong>No hay productos</strong>
{{/if}}